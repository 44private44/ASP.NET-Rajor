@{
}

@model IEnumerable<MissionAdminViewModel>

@foreach (var Mission in Model)
{
    <tr>
        <td>@Mission.title</td>
        <td>@Mission.mission_type</td>
        <td>@Mission.start_date</td>
        <td>@Mission.end_date</td>
        <td>
            <i class="fa-regular fa-pen-to-square UserEditAdminButton" data-userid="@Mission.mission_id"></i>
            <i class="fa-solid fa-trash-can deleteihover UserDeleteAdminButton" data-userid="@Mission.mission_id"></i>
        </td>
    </tr>
}


